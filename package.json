{
  "author": "Dario Sechi",
  "dependencies": {
    "google-auth-library": "^9.4.1",
    "http_ece": "^1.1.0",
    "long": "^5.2.3",
    "protobufjs": "^7.2.5"
  },
  "devDependencies": {
    "@aracna/core": "^1.1.77",
    "@microsoft/api-extractor": "^7.39.1",
    "@types/node": "^20.10.6",
    "@vitest/coverage-v8": "^1.1.1",
    "@vitest/ui": "^1.1.1",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.11",
    "glob": "^10.3.10",
    "prettier": "^3.1.1",
    "protobufjs-cli": "^1.1.2",
    "ts-proto": "^1.166.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.6",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "vitest": "^1.1.1"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs",
    "types": "./index.d.ts"
  },
  "license": "MIT",
  "main": "index.cjs",
  "module": "index.js",
  "name": "@aracna/fcm",
  "peerDependencies": {
    "@aracna/core": ">= 1.1.0"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "printWidth": 160,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "repository": "https://github.com/aracna/fcm",
  "scripts": {
    "build": "rm -rf dist && node esbuild.mjs && cp -rf src/assets dist/assets && pnpm tsc && pnpm api-extractor run --local",
    "generate:proto-definitions": "node scripts/generate-proto-definitions.mjs && pnpm prettier --write src/definitions/proto",
    "generate:proto-ts": "node scripts/generate-proto-ts.mjs && pnpm prettier --write src/assets",
    "prepublish": "pnpm test && pnpm build && pnpm version patch && cp LICENSE package.json README.md dist",
    "test": "vitest run"
  },
  "sideEffects": false,
  "type": "module",
  "types": "index.d.ts",
  "version": "1.0.18"
}
